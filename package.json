{
  "name": "taxia-desktop",
  "productName": "Taxia",
  "version": "1.0.0",
  "type": "module",
  "description": "Tax office workflow management system with automated call transcription and task extraction",
  "main": ".vite/build/index.js",
  "scripts": {
    "start": "npm run build:main && npm run build:preload && concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev": "npm run build:main && npm run build:preload && concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "start:forge": "electron-forge start",
    "dev:vite": "vite --port 5173",
    "dev:electron": "wait-on http://localhost:5173 && electron .vite/build/index.js",
    "build:main": "vite build --config vite.main.config.ts",
    "build:preload": "vite build --config vite.preload.config.ts",
    "build:icons": "./scripts/generate-icons.sh",
    "clean": "rm -rf .vite dist dist-renderer out",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:mac": "electron-forge make --platform=darwin",
    "make:win": "electron-forge make --platform=win32",
    "make:all": "electron-forge make --platform=darwin,win32",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Joon Nam",
    "email": "jungeol66104@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "electron": "^38.2.1",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^5.4.20",
    "wait-on": "^9.0.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "bcryptjs": "^3.0.2",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^11.0.0",
    "express": "^5.1.0",
    "googleapis": "^160.0.0",
    "jsonwebtoken": "^9.0.2",
    "localtunnel": "^2.0.2",
    "lucide-react": "^0.544.0",
    "music-metadata": "^11.9.0",
    "openai": "^5.22.0",
    "prisma": "^6.16.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1"
  }
